{
  "name": "mew-utils",
  "version": "0.0.1",
  "description": "useful collections of javascript written by typescript",
  "browser": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint:fix": "npx eslint src/**/*.ts --config .eslintrc.js --fix",
    "test:unit": "jest --coverage"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,json,md}": [
      "eslint --config .eslintrc.js",
      "git add"
    ]
  },
  "keywords": [
    "utils",
    "mew-utils"
  ],
  "homepage": "https://github.com/CiroLee/mew-utils",
  "bugs": {
    "url": "https://github.com/CiroLee/mew-utils/issues"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/CiroLee/mew-utils.git"
  },
  "author": "cirolee<ciromail@126.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "babel-jest": "^27.5.1",
    "commitlint": "^16.3.0",
    "eslint": "^8.17.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^11.2.6",
    "prettier": "^2.7.1",
    "ts-jest": "^27.1.5",
    "tsup": "^5.12.9",
    "typescript": "^4.7.3"
  }
}
