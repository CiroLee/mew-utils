  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/CiroLee/mew-utils.git"
  },
  "author": "cirolee<ciromail@126.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/config-conventional": "^17.1.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.15",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "commitlint": "^17.1.2",
    "eslint": "^8.23.0",
    "husky": "^8.0.1",
    "jest": "^29.0.2",
    "jest-environment-jsdom": "^29.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2"
  }
}
